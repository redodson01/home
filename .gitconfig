[commit]
	gpgSign = true
[core]
	excludesFile = ~/.gitignore
	pager = diff-so-fancy | less --tabs=4 -RF
[fetch]
	prune = true
	pruneTags = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	process = git-lfs filter-process
	required = true
	smudge = git-lfs smudge -- %f
[init]
	defaultBranch = main
[interactive]
	diffFilter = diff-so-fancy --patch
